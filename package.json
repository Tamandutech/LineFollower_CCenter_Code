    "brotli": "^1.3.2",
    "chart.js": "^4.2.0",
    "dotenv": "^16.0.2",
    "firebase": "^9.9.3",
    "mitt": "^3.0.0",
    "pinia": "^2.0.13",
    "quasar": "^2.6.0",
    "uuid": "^9.0.0",
