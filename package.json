{
  "name": "lf-dash",
  "version": "0.0.1",
  "description": "Dashboard para família Braia de robôs seguidores de linha.",
  "productName": "LF Dash",
  "author": "Raphael Henrique Nunes de Freitas <raphaeldefreitas2009@gmail.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "quasar dev",
    "build": "quasar build -m pwa",
    "deploy:stage": "firebase deploy -P tt-linefollower-dev",
    "deploy:prod": "firebase deploy -P tt-linefollower",
    "lint": "pnpm lint:ci --fix",
    "lint:ci": "eslint --ext .js,.ts,.vue ./",
    "format": "pnpm format:ci --write",
    "format:ci": "prettier --check \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/cli": "^2.4.1",
    "@quasar/extras": "^1.16.12",
    "@vueuse/core": "^11.1.0",
    "@vueuse/firebase": "^11.1.0",
    "chart.js": "^4.4.4",
    "firebase": "^10.14.0",
    "mitt": "^3.0.1",
    "pinia": "^2.2.4",
    "quasar": "^2.17.0",
    "register-service-worker": "^1.7.2",
    "uuid": "^10.0.0",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/web-bluetooth": "^0.0.20",
    "@quasar/app-vite": "^2.0.0-beta.12",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "typescript": "~5.5.2",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6",
    "workbox-build": "^7.1.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
